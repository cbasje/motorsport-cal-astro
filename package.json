{
    "name": "motorsport-cal-astro",
    "type": "module",
    "version": "0.0.1",
    "description": "A calendar for motorsport",
    "author": "Sebastiaan Benjamins",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "astro dev",
        "dev:db": "fly proxy 5432 -a motorsport-cal-db",
        "start": "node dist/server/entry.mjs",
