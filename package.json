{
	"name": "motorsport-cal-astro",
	"type": "module",
	"version": "0.0.1",
	"description": "A calendar for motorsport",
	"author": "Sebastiaan Benjamins",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "astro dev --port 3000",
		"dev:db": "fly proxy 5432 -a motorsport-cal-db",
		"start": "bun run ./dist/server/entry.mjs",
		"preview": "bun run build && bun run start",
		"build": "astro check && astro build",
		"astro": "astro",
		"db:migrate": "bun run ./drizzle/migrate.ts",
		"db:seed": "bun run ./drizzle/seed.ts",
		"db:push": "drizzle-kit push:pg",
		"db:pull": "drizzle-kit introspect:pg",
		"db:drop": "drizzle-kit drop",
		"db:generate": "drizzle-kit generate:pg",
		"db:up": "drizzle-kit up:pg",
		"db:check": "drizzle-kit check:pg",
		"db:studio": "drizzle-kit studio",
		"deploy": "fly deploy"
	},
	"dependencies": {
		"@astrojs/check": "^0.3.4",
		"@astrojs/svelte": "^4.0.0",
		"@iconify-json/fluent-emoji-high-contrast": "^1.1.9",
		"@sveltestack/svelte-query": "^1.6.0",
		"astro": "3.6.4",
		"astro-icon": "latest",
		"astro-seo": "^0.8.0",
		"drizzle-orm": "^0.29.2",
		"drizzle-zod": "^0.5.1",
		"ics": "^3.1.0",
		"maplibre-gl": "^3.2.1",
		"nanostores": "^0.9.3",
		"open-props": "^1.6.8",
		"pg": "^8.11.3",
		"sharp": "0.33.0-alpha.9",
		"simplex-noise": "^4.0.1",
		"superjson": "^1.12.3",
		"svelte": "^4.2.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^2.0.0",
		"@faker-js/faker": "^8.3.1",
		"@flydotio/dockerfile": "^0.4.10",
		"@iconify/types": "^2.0.0",
		"@types/node": "^18.15.11",
		"@types/pg": "^8.10.9",
		"autoprefixer": "^10.4.14",
		"bun-types": "^1.0.2",
		"cssnano": "^6.0.1",
		"drizzle-kit": "^0.20.9",
		"postcss": "^8.4.31",
		"postcss-custom-media": "^10.0.2",
		"postcss-nested": "^6.0.1",
		"prettier": "^3.1.1",
		"prettier-plugin-astro": "^0.12.3",
		"typescript": "^5.3.3",
		"zod-prisma": "^0.5.4"
	},
	"trustedDependencies": [
		"sharp"
	]
}
