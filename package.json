        "@trpc/client": "^10.38.1",
        "@trpc/server": "^10.38.1",
        "astro": "^3.0.5",
        "astro-icon": "^1.0.0-next.2",
        "astro-seo": "^0.8.0",
        "ics": "^3.1.0",
        "maplibre-gl": "^3.2.1",
        "prisma": "^5.2.0",
        "sharp": "^0.32.4",
        "superjson": "^1.12.3",
        "svelte": "^4.2.0",
        "zod": "^3.21.4"
